<ol class="post-list">
  {{#each posts as |post|}}
  <li>
    {{> post-info post }}
  </li>
  {{/each}}
</ol>


{{#each post}}
<div class="post-div">
    <h3 class = "post-title">{{title}}</h3>
    <h4 class = "post-user">Posted by: {{username}}</h4>
    <p class = "post-content">{{text}}</p>
</div>
{{/each}}